{
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.node.tsbuildinfo",
    "skipLibCheck": true,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "strict": true,
    "noEmit": false,
    "baseUrl": ".",
    "paths": {
      "$lib": ["resources/lib"],
      "$lib/*": ["resources/lib/*"]
    }
  },
  "include": ["vite.config.ts"]
}
