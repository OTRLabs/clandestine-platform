#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user/platform/conti-platform/node_modules/.pnpm/@gcornut+valibot-json-schema@0.31.0/node_modules/@gcornut/valibot-json-schema/bin/node_modules:/home/user/platform/conti-platform/node_modules/.pnpm/@gcornut+valibot-json-schema@0.31.0/node_modules/@gcornut/valibot-json-schema/node_modules:/home/user/platform/conti-platform/node_modules/.pnpm/@gcornut+valibot-json-schema@0.31.0/node_modules/@gcornut/node_modules:/home/user/platform/conti-platform/node_modules/.pnpm/@gcornut+valibot-json-schema@0.31.0/node_modules:/home/user/platform/conti-platform/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/user/platform/conti-platform/node_modules/.pnpm/@gcornut+valibot-json-schema@0.31.0/node_modules/@gcornut/valibot-json-schema/bin/node_modules:/home/user/platform/conti-platform/node_modules/.pnpm/@gcornut+valibot-json-schema@0.31.0/node_modules/@gcornut/valibot-json-schema/node_modules:/home/user/platform/conti-platform/node_modules/.pnpm/@gcornut+valibot-json-schema@0.31.0/node_modules/@gcornut/node_modules:/home/user/platform/conti-platform/node_modules/.pnpm/@gcornut+valibot-json-schema@0.31.0/node_modules:/home/user/platform/conti-platform/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@gcornut/valibot-json-schema/bin/index.js" "$@"
else
  exec node  "$basedir/../@gcornut/valibot-json-schema/bin/index.js" "$@"
fi
